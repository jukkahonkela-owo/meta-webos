 files/launch/filecache.conf.in |   30 ++++++++++++++++++++++++++++++
 files/launch/filecache.in      |   30 ------------------------------
 2 files changed, 30 insertions(+), 30 deletions(-)
 create mode 100644 files/launch/filecache.conf.in
 delete mode 100644 files/launch/filecache.in

diff --git a/files/launch/filecache.conf.in b/files/launch/filecache.conf.in
new file mode 100644
index 0000000..35d3f6c
--- /dev/null
+++ b/files/launch/filecache.conf.in
@@ -0,0 +1,30 @@
