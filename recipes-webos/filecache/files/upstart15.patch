+
+start on stopped finish
+stop on started start_update
+
+respawn
+exec @WEBOS_INSTALL_SBINDIR@/filecache -c {\"log\":{\"appender\":{\"type\":\"syslog\"},\"levels\":{\"default\":\"warning\"}}}
+
+pre-start script
+  if grep -qs "qemux86" /etc/hostname ; then
+    mount / -oremount,user_xattr 
+  fi      
+end script
